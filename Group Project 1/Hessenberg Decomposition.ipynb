   "source": [
    "## Perform a Hessenberg decomposition\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "m = 6\n",
    "A = rand((-1.0:1.0), m, m)\n",
    "H = A\n",
    "for i in 1:m-1\n",
    "    u = H[:,i]\n",
    "    u[1:i] *= 0.0; \n",
    "    u[i + 1] -= norm(u)\n",
    "    u = normalize(u)\n",
    "    Q = I - 2*kron(u, u)\n",
    "    H = Q*H*Q'\n",
    "end \n",
    "H"
   ]
  },
